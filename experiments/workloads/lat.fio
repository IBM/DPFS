[global]
name=fio-lat-read
filename=${MNT}/fio-10g
size=10G

ioengine=io_uring
# Not supported with virtio-fs
# registerfiles=1
# sqthread_poll=1
# hipri=1
# fixedbufs=1

bs=1
iodepth=1
direct=1
numjobs=1

time_based
group_reporting
lat_percentiles=1
norandommap=1

[read]
stonewall
rw=read
ramp_time=5s
runtime=15s

[write]
stonewall
rw=write
ramp_time=5s
runtime=15s
